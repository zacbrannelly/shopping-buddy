    {
      "recipe": {
        "id": "138e83fc-6aed-4727-8736-a36da78d182c",
        "name": "Spaghetti Carbonara",
        "type": "Italian",
        "image": "spaghetti.jpg",
        "isImageAsset": true
      },
      "steps": [
        {
          "step": 0,
          "description": "In a large pot of boiling salted water, cook pasta according to package instructions; reserve 1/2 cup water and drain well."
        },
        {
          "step": 1,
          "description": "In a small bowl, whisk together eggs and Parmesan; set aside."
        },
        {
          "step": 2,
          "description": "Heat a large skillet over medium high heat. Add bacon and cook until brown and crispy, about 6-8 minutes; reserve excess fat."
        },
        {
          "step": 3,
          "description": "Stir in garlic until fragrant, about 1 minute. Reduce heat to low."
        },
        {
          "step": 4,
          "description": "Working quickly, stir in pasta and egg mixture, and gents toss to combine; season with salt and pepper, to taste. Add reserved pasta water, one tablespoon at a time, until desired consistency is reached."
        },
        {
          "step": 5,
          "description": "Serve immediately, garnished with parsley, if desired."
        }
      ],
